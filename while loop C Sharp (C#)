using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace hello
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int i1 = 1;
            while (i1 <= 10)
            {
                Console.WriteLine($"5 * {i1} = {5*i1}");
                i1++;
            }

           
            Console.ReadKey();
        }
    }
}
