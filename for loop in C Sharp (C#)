using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace hello
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int i2;
            i2 = int.Parse(Console.ReadLine());

            for (int i = 0; i <= i2; i++)
            {
                if (i % 2 == 0)
                {
                    Console.WriteLine(i);
                }
            }


            Console.ReadKey();
        }
    }
}
